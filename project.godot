; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Five Parsecs Campaign Manager"
run/main_scene="res://ui/mainscene/Main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

SaveGame="*res://Scenes/Management/SaveManager.gd"
Signal_Debugger="*res://addons/SignalVisualizer/Debugger/SignalDebugger.gd"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/SignalVisualizer/plugin.cfg", "res://addons/block_code/plugin.cfg")

[file_customization]

folder_colors={
"res://Scenes/": "blue",
"res://Scenes/Management/": "yellow",
"res://Scenes/Utils/": "green",
"res://Scenes/campaign/": "red",
"res://Scripts/": "purple",
"res://ui/mainmenu/": "orange"
}

[maaacks_menus_template]

disable_plugin_dialogues=true
copy_path="res://Assets"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
