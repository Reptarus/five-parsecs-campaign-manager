{
    "dirs": [
        "res://tests/unit",
        "res://tests/integration",
        "res://tests/performance",
        "res://tests/mobile"
    ],
    "double_strategy": "partial",
    "ignore_pause": true,
    "include_subdirs": true,
    "log_level": 3,
    "prefix": "test_",
    "should_exit": true,
    "should_maximize": true,
    "should_exit_on_success": true,
    "export_path": "res://tests/reports/",
    "junit_xml_file": "res://tests/reports/results.xml",
    "junit_xml_timestamp": true,
    "inner_class_name": "Test",
    "opacity": 100,
    "selected": "",
    "suffix": ".gd",
    "tests": [],
    "unit_test_name": "",
    "gut_on_top": true,
    "should_print_to_console": true,
    "compact_mode": false,
    "show_help": false,
    "pre_run_script": "res://tests/fixtures/setup_gut.gd",
    "post_run_script": "res://tests/fixtures/cleanup_gut.gd",
    "color_output": true,
    "log_file": "res://tests/logs/gut_run.log",
    "directory_filter": ".*",
    "file_filter": "test_.*\\.gd$",
    "include_subdirectories": true,
    "inner_class": "Test",
    "junit_xml_dir": "res://tests/reports/",
    "junit_xml_prefix": "",
    "junit_xml_timestamp_format": "%Y_%m_%d_%H_%M_%S",
    "log_format": "[{timestamp}] {level}: {message}",
    "log_timestamp_format": "%Y-%m-%d %H:%M:%S",
    "disable_strict_datatype_checks": false,
    "should_maximize": true,
    "should_use_colors": true,
    "hide_orphans": true,
    "errors_cause_failure": true
