    "dirs": [
        "res://tests/unit/",
        "res://tests/integration/"
    ],
    "include_subdirs": true,
    "ignore_pause": true,
    "log_level": 3,
    "should_exit": false,
    "should_maximize": true,
    "prefix": "test_",
